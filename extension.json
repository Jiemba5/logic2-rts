{
  "name": "freeRTS",
  "apiVersion": "1.0.0",
  "author": "jiemba5",
  "version": "0.0.2",
  "description": "Includes measures for finding utilization data for a sequential Real-Time Systems Task scheduler.",
  "extensions": {
    "freeRTS": {
      "type": "DigitalMeasurement",
      "entryPoint": "DigitalMeasurement.MyDigitalMeasurer",
      "metrics": {
        "c_average": {
          "name": "Average c",
          "notation": "C<sub>average</sub>",
          "units": "s"
        },
        "c_worst": {
          "name": "Worst c",
          "notation": "C<sub>worst</sub>",
          "units": "s"
        },
        "c_best": {
          "name": "Best c",
          "notation": "C<sub>best</sub>",
          "units": "s"
        },
        "c_std": {
          "name": "Std c",
          "notation": "C<sub>std</sub>",
          "units": "s"
        },
        "t_complete": {
          "name": "Tasks completed",
          "notation": "Tasks completed"
        },
        "p_average": {
          "name": "Average task period",
          "notation": "T<sub>mean</sub>",
          "units": "s"
        }
      }
    }
  }
}
